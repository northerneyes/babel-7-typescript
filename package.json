{
  "name": "babel-7-typescript-example",
  "version": "1.0.0",
  "description": "Example TypeScript project built on top of new Babel 7 features",
  "keywords": [
    "babel",
    "babel 7",
    "typescript",
    "react",
    "example"
  ],
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development babel-node --extensions '.ts,.tsx' ./server/index.ts",
    "build": "yarn clean:build && babel src --ignore src/**/__tests__ --out-dir dist --extensions '.ts,.tsx'",
    "build:client": "NODE_ENV=development webpack --config ./webpack.dev.config.js",
    "build:dll": "webpack --config ./webpack.dll.config.js",
    "build:server": "yarn clean:build && babel server --ignore src/**/__tests__ --out-dir dist --extensions '.ts,.tsx'",
    "clean:build": "rm -rf dist",
    "coverage": "jest --coverage",
    "format": "prettier --write \"{webpack,src,server}/**/*.{js,ts,tsx}\" ",
    "precommit": "lint-staged",
    "typecheck": "tsc",
    "lint": "tslint -c tslint.json --project tsconfig.json -t codeFrame",
    "prepublishOnly": "yarn build",
    "start": "babel-node --extensions '.ts,.tsx' index.ts",
    "test": "jest",
    "test:watch": "jest -o --watch"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "tslint -c tslint.json --project tsconfig.json -t codeFrame",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/friendly-errors-webpack-plugin": "^0.1.1",
    "@types/jest": "^21.1.8",
    "@types/react": "16.4.2",
    "@types/react-dom": "^16.0.7",
    "@types/webpack": "^4.4.11",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.4",
    "babel-loader": "^8.0.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "friendly-errors-webpack-plugin": "1.6.1",
    "husky": "0.13.4",
    "jest": "21.3.0-beta.9",
    "lint-staged": "3.6.1",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "react-hot-loader": "^4.3.6",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-language-service": "^0.9.9",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "typestrict": "^1.0.0",
    "webpack": "^4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.15.0"
  },
  "dependencies": {
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/node": "8.10.29",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "react": "16.4.2",
    "react-dom": "16.4.2"
  }
}
